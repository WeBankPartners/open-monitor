<?xml version="1.0" encoding="UTF-8" ?>
<package name="wecube-monitor" version="{{PLUGIN_VERSION}}">

    <!-- 1.依赖分析 - 描述运行本插件包需要的其他插件包 -->
    <packageDependencies>
        <packageDependency name="wecmdb" version="v1.5.0"/>
        <packageDependency name="saltstack" version="v1.9.0"/>
        <packageDependency name="platform" version="v2.5.0"/>
    </packageDependencies>

    <!-- 2.菜单注入 - 描述运行本插件包需要注入的菜单 -->
    <menus>
        <menu code='MONITOR_MAIN_DASHBOARD' cat='MONITORING' displayName="Monitor Dashboard" localDisplayName="对象视图">/endpointView</menu>
        <menu code='MONITOR_ALARM_CONFIG' cat='MONITORING' displayName="Monitor Config" localDisplayName="监控配置">/monitorConfigIndex/endpointManagement</menu>
        <menu code='MONITOR_METRIC_CONFIG' cat='MONITORING' displayName="Metric Design" localDisplayName="指标设计">/metricConfig</menu>
        <menu code='MONITOR_ALARM_MANAGEMENT' cat='MONITORING' displayName="Alarm List" localDisplayName="告警列表">/alarmManagement</menu>
        <menu code='MONITOR_CUSTOM_DASHBOARD' cat='MONITORING' displayName="Custom Dashboard" localDisplayName="自定义视图">/viewConfigIndex</menu>
    </menus>

    <!-- 3.数据模型 - 描述本插件包的数据模型,并且描述和Framework数据模型的关系 -->
    <dataModel>
        <entity name="alarm" displayName="告警列表" description="告警列表模型">
            <attribute name="id" datatype="str" description="唯一ID"/>
            <attribute name="status" datatype="str" description="状态"/>
            <attribute name="subject" datatype="str" description="主题"/>
            <attribute name="content" datatype="str" description="内容"/>
            <attribute name="to" datatype="str" description="接收人"/>
            <attribute name="toMail" datatype="str" description="接收邮箱"/>
            <attribute name="toPhone" datatype="str" description="接收手机号"/>
            <attribute name="toRole" datatype="str" description="接收角色"/>
        </entity>
    </dataModel>

    <!-- 4.系统参数 - 描述运行本插件包需要的系统参数 -->
    <systemParameters>
        <systemParameter name="MYSQL_MONITOR_USER" scopeType="global" defaultValue="exporter"/>
        <systemParameter name="MYSQL_MONITOR_PWD" scopeType="global" defaultValue="prom_pwd"/>
        <systemParameter name="MYSQL_MONITOR_DATABASE" scopeType="global" defaultValue="mysql"/>
        <systemParameter name="REDIS_MONITOR_USER" scopeType="global" defaultValue="exporter"/>
        <systemParameter name="REDIS_MONITOR_PWD" scopeType="global" defaultValue="prom_pwd"/>
        <systemParameter name="JMX_MONITOR_USER" scopeType="global" defaultValue="exporter"/>
        <systemParameter name="JMX_MONITOR_PWD" scopeType="global" defaultValue="prom_pwd"/>
        <systemParameter name="MONITOR_HOST_GROUP" scopeType="plugins" defaultValue="default_host_group"/>
        <systemParameter name="MONITOR_MYSQL_GROUP" scopeType="plugins" defaultValue="default_mysql_group"/>
        <systemParameter name="MONITOR_REDIS_GROUP" scopeType="plugins" defaultValue="default_redis_group"/>
        <systemParameter name="MONITOR_TOMCAT_GROUP" scopeType="plugins" defaultValue="default_tomcat_group"/>
        <systemParameter name="MONITOR_NGINX_GROUP" scopeType="plugins" defaultValue="default_nginx_group"/>
        <systemParameter name="MONITOR_JAVA_GROUP" scopeType="plugins" defaultValue="default_java_group"/>
        <systemParameter name="HOST_EXPORTER_S3_PATH" scopeType="global" defaultValue="http://127.0.0.1:9000/wecube-agent/node_exporter.tar.gz"/>
        <systemParameter name="MONITOR_MAIL_SENDER_USER" scopeType="global" defaultValue="default_user"/>
        <systemParameter name="MONITOR_MAIL_SENDER_SERVER" scopeType="global" defaultValue="default_server"/>
        <systemParameter name="MONITOR_MAIL_SENDER_PASSWORD" scopeType="global" defaultValue="default_password"/>
        <systemParameter name="MONITOR_ALARM_FIRING_CALLBACK" scopeType="plugins" defaultValue="default_firing_callback"/>
        <systemParameter name="MONITOR_ALARM_RECOVER_CALLBACK" scopeType="plugins" defaultValue="default_recover_callback"/>
        <systemParameter name="HOST_EXPORTER_INSTALL_SCRIPT" scopeType="global" defaultValue="/tmp/monitor/node_exporter/start.sh"/>
        <systemParameter name="HOST_EXPORTER_UNPACK" scopeType="global" defaultValue="true"/>
        <systemParameter name="HOST_EXPORTER_UPLOAD_PATH" scopeType="global" defaultValue="/tmp/monitor/"/>
        <systemParameter name="MONITOR_PING_GROUP" scopeType="plugins" defaultValue="default_ping_group"/>
        <systemParameter name="MONITOR_TELNET_GROUP" scopeType="plugins" defaultValue="default_telnet_group"/>
        <systemParameter name="MONITOR_DASHBOARD_DELETE_ALL" scopeType="plugins" defaultValue="Y"/>
        <systemParameter name="MONITOR_DASHBOARD_NOT_DELETE_ALL" scopeType="plugins" defaultValue="N"/>
        <systemParameter name="MONITOR_SCRAPE_STEP_1" scopeType="plugins" defaultValue="1"/>
        <systemParameter name="MONITOR_SCRAPE_STEP_5" scopeType="plugins" defaultValue="5"/>
        <systemParameter name="MONITOR_SCRAPE_STEP_10" scopeType="plugins" defaultValue="10"/>
        <systemParameter name="MONITOR_SCRAPE_STEP_30" scopeType="plugins" defaultValue="30"/>
        <systemParameter name="MONITOR_SCRAPE_STEP_60" scopeType="plugins" defaultValue="60"/>
        <systemParameter name="MONITOR_HTTP_METHOD_GET" scopeType="global" defaultValue="get"/>
        <systemParameter name="MONITOR_HTTP_METHOD_POST" scopeType="global" defaultValue="post"/>
        <systemParameter name="MONITOR_CHECK_EVENT_KEY" scopeType="global" defaultValue=""/>
        <systemParameter name="MONITOR_CHECK_EVENT_TO_MAIL" scopeType="global" defaultValue=""/>
        <systemParameter name="MONITOR_CHECK_EVENT_INTERVAL_MIN" scopeType="global" defaultValue="60"/>
        <systemParameter name="MONITOR_ARCHIVE_ENABLE" scopeType="global" defaultValue="Y"/>
        <systemParameter name="MONITOR_ARCHIVE_MYSQL_HOST" scopeType="global" defaultValue=""/>
        <systemParameter name="MONITOR_ARCHIVE_MYSQL_PORT" scopeType="global" defaultValue="3306"/>
        <systemParameter name="MONITOR_ARCHIVE_MYSQL_USER" scopeType="global" defaultValue=""/>
        <systemParameter name="MONITOR_ARCHIVE_MYSQL_PWD" scopeType="global" defaultValue=""/>
        <systemParameter name="MONITOR_LOG_LEVEL" scopeType="global" defaultValue="info"/>
    </systemParameters>


    <!-- 5.权限设定 -->
    <authorities>
        <authority systemRoleName="SUPER_ADMIN" >
            <menu code="MONITOR_MAIN_DASHBOARD" />
            <menu code="MONITOR_METRIC_CONFIG" />
            <menu code="MONITOR_CUSTOM_DASHBOARD" />
            <menu code="MONITOR_ALARM_CONFIG" />
            <menu code="MONITOR_ALARM_MANAGEMENT" />
