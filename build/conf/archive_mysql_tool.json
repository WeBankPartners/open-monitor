{
  "enable": "{{MONITOR_ARCHIVE_ENABLE}}",
  "type": "{{MONITOR_ARCHIVE_TYPE}}",
  "mysql": {
    "server": "{{MONITOR_ARCHIVE_MYSQL_HOST}}",
    "port": "{{MONITOR_ARCHIVE_MYSQL_PORT}}",
    "user": "{{MONITOR_ARCHIVE_MYSQL_USER}}",
    "password": "{{MONITOR_ARCHIVE_MYSQL_PWD}}",
    "database_prefix": "prometheus_archive_",
    "max_open": 150,
    "max_idle": 10,
    "timeout": 60
  },
  "hdfs":{
    "krb5":"/app/monitor/archive_mysql_tool/conf/krb5.conf",
    "keytab":"/app/monitor/archive_mysql_tool/keytab/{{MONITOR_ARCHIVE_HDFS_USER}}.keytab",
    "username":"{{MONITOR_ARCHIVE_HDFS_USER}}",
    "realm":"{{MONITOR_ARCHIVE_KRB_REALM}}",
    "dest_dir":"{{MONITOR_ARCHIVE_HDFS_DEST_DIR}}",
    "conf_dir":"/app/monitor/archive_mysql_tool/conf",
    "local_temp_dir":"/app/monitor/archive_mysql_tool/data",
    "max_file_open": "{{MONITOR_ARCHIVE_MAX_FILE_OPEN}}"
  },
  "prometheus": {
    "server": "127.0.0.1",
    "port": 9090,
    "max_http_open": 100,
    "max_http_idle": 10,
    "http_idle_timeout": 60,
    "query_step": 10,
    "ignore_tags": ["__name__","job"]
  },
  "monitor": {
    "mysql": {
      "server": "{{MONITOR_DB_HOST}}",
      "port": "{{MONITOR_DB_PORT}}",
      "user": "{{MONITOR_DB_USER}}",
      "password": "{{MONITOR_DB_PWD}}",
      "database": "monitor",
      "max_open": 10,
      "max_idle": 2,
      "timeout": 60
    }
  },
  "trans": {
    "max_unit_speed": 5,
    "five_min_start_day": 90,
    "concurrent_insert_num": 100,
    "retry_wait_second": 60
  },
  "http": {
    "enable": true,
    "port": 9097
  }
}