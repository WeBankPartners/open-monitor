{
  "debug": false,
  "interval": 30,
  "ping_enable": true,
  "telnet_enable": true,
  "open-falcon" : {
    "enabled": false,
    "transfer": {
      "address": [
        "192.168.0.1:8080"
      ],
      "timeout": 1000,
      "sn": 500
    }
  },
  "prometheus" : {
    "enabled" : true,
    "port": "9191",
    "path": "/metrics"
  },
  "source": {
    "const": {
      "enabled" : true,
      "ips": [],
      "weight": 1
    },
    "file": {
      "enabled" : true,
      "path": "ip.txt",
      "weight": 2
    },
    "remote": {
      "enabled" : true,
      "header": ["X-Auth-Token=default-token-used-in-server-side"],
      "group_tag" : "group=g1",
      "url": "http://192.168.67.133:8088/wecube-monitor/api/v1/agent/export/ping/source",
      "interval": 60,
      "weight": 3
    },
    "listen": {
      "enabled" : false,
      "port": "9191",
      "path": "/config/ip",
      "weight": 4
    }
  },
  "metrics": {
    "ping": "ping_alive",
    "ping_count_num": "ping_count",
    "ping_count_success": "ping_success",
    "ping_count_fail": "ping_fail",
    "telnet": "telnet_alive",
    "telnet_count_num": "telnet_count",
    "telnet_count_success": "telnet_success",
    "telnet_count_fail": "telnet_fail"
  }
}