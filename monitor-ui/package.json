{
  "name": "web-vuecli3.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "node node-api-config.js && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "plugin": "node node-api-config.js && vue-cli-service build --mode plugin"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/js-cookie": "^2.2.6",
    "@vue/cli-plugin-typescript": "^5.0.9",
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "colresizable": "^1.6.0",
    "core-js": "^2.6.5",
    "cryptiles": "^4.1.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.12",
    "echarts": "^4.2.1",
    "hoek": "^4.2.1",
    "jquery": "^3.4.1",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.1",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.21",
    "serialize-javascript": "^3.1.0",
    "style-resources-loader": "^1.2.1",
    "typescript": "^3.9.2",
    "vee-validate": "^2.0.6",
    "view-design": "4.3.2",
    "vue": "^2.6.12",
    "vue-cli-plugin-iview": "^2.0.0",
    "vue-ellipse-progress": "^1.3.1",
    "vue-grid-layout": "2.1.13",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "wecube-common-ui": "^1.0.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^5.0.9",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^5.2.3",
    "font-awesome": "^4.7.0",
    "husky": "^1.3.1",
    "less": "^3.13.1",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.12",
    "worker-loader": "^3.0.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
